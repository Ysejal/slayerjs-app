    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.3",
    "loadash": "^1.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
